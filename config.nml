&io_control
    initial_filename = 'initial.nc'
    save_restart_file = T
    restart_filename = 'output/restart.nc'
/

&time_control
    max_timestep = "PT10S"
    run_duration = "PT48H"
    output_interval = "PT10M"
/

&physics_parameters
    g = 9.807 ! acceleration due to gravity, m/s2
    f = 1e-5  ! coriolis parameter, 1/s
/
